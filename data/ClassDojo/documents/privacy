<!DOCTYPE html><html lang="en-US"><head><meta charSet="utf-8"/><meta http-equiv="x-ua-compatible" content="ie=edge"/><meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"/><script type="text/javascript" crossorigin="anonymous" src="/browsersupportblocker.cd9f1603.min.js"></script><script type="text/javascript" src="/set-on-error-8.js"></script><script type="text/javascript">
    (function() {
      "use strict";

      const variants = {
          control: [0.00, 0.20],
          v1: [0.333, 0.533],
          v2: [0.667, 0.867],
      };

      const SALT = "0.6098119324025844";
      const SESSION_COOKIE_NAME = "dojo_log_session_id";
      const SAVED_VARIANT_NAME = "web_external_homepage_experiment_2025_v1b";
      const VALID_PATHS = ['/', '/v1/', '/v2/'];

      async function initVariantSelection() {

          if ((window.location.hostname === "www.classdojo.com" || window.location.hostname === "www2.classdojo.com" || window.location.hostname === "www3.classdojo.com") && navigator.language === "en-US" && VALID_PATHS.includes(window.location.pathname)) {
              let variantName = getCookie(SAVED_VARIANT_NAME);
              let sessionId = getCookie(SESSION_COOKIE_NAME);
              if (!variantName) {
                  try {
                      if (!sessionId) {
                          sessionId = crypto.randomUUID();
                          let expires = new Date();
                          expires.setFullYear(expires.getFullYear() + 1);
                          const expiresStr = expires.toUTCString();
                          setCookie(SESSION_COOKIE_NAME, sessionId, { path: '/', expires: expiresStr, secure: true });
                      }
                      variantName = await chooseVariantForId(sessionId, variants, SALT);

                      let expires = new Date();
                      expires.setFullYear(expires.getFullYear() + 1);
                      const expiresStr = expires.toUTCString();
                      setCookie(SAVED_VARIANT_NAME, variantName, { path: '/', expires: expiresStr, secure: true });
                  } catch (error) {
                      console.error("Error while selecting variant:", error);
                      return;
                  }
              }

              switch (variantName) {
                  case "v1":
                      if (window.location.pathname !== "/v1/") {
                          window.location.href = "/v1/";
                      }
                      break;
                  case "v2":
                      if (window.location.pathname !== "/v2/") {
                          window.location.href = "/v2/";
                      }
                      break;
                  case "control":
                  case "off":
                      if (window.location.pathname !== "/") {
                          window.location.href = "/";
                      }
                      break;
                  default:
                      break;
              }
          }
      }

      initVariantSelection();

      function getCookie(name) {
          const value = `;${document.cookie}`;
          const parts = value.split(`; ${name}=`);
          if (parts.length === 2) {
              return parts.pop().split(';').shift();
          }
          return null;
      }

      function setCookie(name, value, options = {}) {
          let optionsString = Object.keys(options).map(key => `${key}=${options[key]}`).join('; ');
          document.cookie = `${name}=${value}; ${optionsString}`;
      }

      async function chooseVariantForId(id, variants, salt) {
          const encoder = new TextEncoder();
          const data = encoder.encode(`${id}:${salt}`);
          const hashBuffer = await crypto.subtle.digest('SHA-1', data);
          const hashArray = Array.from(new Uint8Array(hashBuffer));
          const hashHex = hashArray.map(b => b.toString(16).padStart(2, '0')).join('');
          const idAsNumber = BigInt('0x' + hashHex);
          const bucket = Number(idAsNumber % BigInt(10000)) / 10000;
          return getVariantFromBucket(bucket, variants);
      }

      function getVariantFromBucket(bucket, variants) {
          for (let variant in variants) {
              let range = variants[variant];
              if (bucket >= range[0] && bucket < range[1]) {
                  console.log("Variant found:", variant);
                  return variant;
              }
          }
          return 'off';
      }

  })();
